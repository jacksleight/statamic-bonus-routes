{
    "name": "jacksleight/statamic-bonus-routes",
    "autoload": {
        "psr-4": {
            "JackSleight\\StatamicBonusRoutes\\": "src"
        }
    },
    "extra": {
        "statamic": {
            "name": "Bonus Routes",
            "description": "This Statamic addon simplifies setting up additional collection and taxonomy based routes by handling the dynamic mounting and data retrieval for you."
        },
        "laravel": {
            "providers": [
                "JackSleight\\StatamicBonusRoutes\\ServiceProvider"
            ]
        }
    },
    "require": {
        "statamic/cms": "^3.3 || ^4.0"
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    }
}
